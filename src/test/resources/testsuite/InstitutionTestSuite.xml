                    <include name="listUsersPositive"/>
                </methods>
            </class>
            <class name="org.ays.tests.institution.usermanagementservice.PutUserTest">
                <methods>
                    <include name="updateUserAsPassive"/>
                    <include name="updateUserAsActive"/>
                    <include name="updateUserWithInvalidRole"/>
                    <include name="updateUserWithBlankRole"/>
